{
  "name": "estalots",
  "version": "1.0.0",
  "description": "Api Desenvolvida Utilizando TS, ES6, Postgres e Sequelize",
  "main": "server/serve.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development ./node_modules/.bin/ts-node ./server/server.ts",
    "builded": "cross-env NODE_ENV=development node ./build/server.js",
    "unit-test": "cross-env NODE_ENV=test mocha --opts build/tests/unit/config/mocha.opts ./build/tests/unit/unit.test.js",
    "integration-test": "cross-env NODE_ENV=test mocha --opts build/tests/integration/config/mocha.opts ./build/tests/integration/integration.test.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Api",
    "TypeScript",
    "Node",
    "ES6"
  ],
  "author": "Rafael Alves",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.17.2",
    "cross-env": "^5.0.5",
    "express": "^4.15.4",
    "http": "0.0.0",
    "morgan": "^1.8.2",
    "pg": "^7.1.2",
    "sequelize": "^4.6.0",
    "sequelize-cli": "^2.8.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/chai": "^4.0.4",
    "@types/express": "^4.0.37",
    "@types/mocha": "^2.2.42",
    "@types/morgan": "^1.7.32",
    "@types/sequelize": "^4.0.69",
    "@types/supertest": "^2.0.3",
    "chai": "^4.1.1",
    "mocha": "^3.5.0",
    "sequelize": "^4.6.0",
    "supertest": "^3.0.0",
    "testdouble": "^3.2.4",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.2"
  }
}
